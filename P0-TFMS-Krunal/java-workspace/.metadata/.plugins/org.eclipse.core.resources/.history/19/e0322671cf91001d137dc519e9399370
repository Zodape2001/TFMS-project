package com.revature.utility;

public class string {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s1 = "krunal";
		String s2="";
		for(int i=0; i<=s1.length()-1; i++) {
			s2+=s1.charAt(i);
		}
		if(s1.equals(s2))
		{
			System.out.println("palindrome");
		}
		else {
			System.out.println("not palindrome");
		}
	}

}
