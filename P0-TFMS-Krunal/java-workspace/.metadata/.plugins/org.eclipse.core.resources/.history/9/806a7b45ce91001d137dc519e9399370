package com.revature.utility;

public class string {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
//		String s1 = "krunal";
//		String s3 = "krunal";
		String s2 = " is a good boy good";
		for(int i=s2.length()-1; i<=0; i--) {
			System.out.print(s2.charAt(i));
		}

	}

}
